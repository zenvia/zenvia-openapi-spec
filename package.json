    "cors": "^2.8.5",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.19.2",
    "filewatcher": "^3.0.1",
    "glob": "^7.1.6",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mocha": "^10.2.0",
    "ncp": "^2.0.0",
    "oas-validator": "^5.0.2",
    "openapi-enforcer": "^1.23.0",
    "openapi3-ts": "^2.0.0",
