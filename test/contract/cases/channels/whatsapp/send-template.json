{
  "name": "send template content",
  "request": {
    "method": "POST",
    "path": "/channels/whatsapp/messages",
    "query": {
    },
    "headers": {
      "content-type": "application/json",
      "x-api-token": "some-api-token"
    },
    "body": {
      "from": "SENDER ID",
      "to": "RECIPIENT ID",
      "contents": [{
        "type": "template",
        "templateId": "some template id",
        "fields": {
          "field1": "field1 value",
          "field2": "field2 value"
        }
      }]
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "content-type": "application/json"
    },
    "body": {
      "id": "CREATED MESSAGE ID",
      "direction": "OUT",
      "from": "SENDER ID",
      "to": "RECIPIENT ID",
      "contents": [{
        "type": "template",
        "templateId": "some template id",
        "fields": {
          "field1": "field1 value",
          "field2": "field2 value"
        }
      }]
    }
  }
}
